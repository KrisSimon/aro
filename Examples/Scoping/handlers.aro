(*
 * ARO-0003 Event Handlers
 * Demonstrates accessing published variables from event chain
 *)

(Log Startup: StartupComplete Handler) {
    (* Access published variables from the event chain *)
    <Log> the <name-message> for the <console> with <application-name>.
    <Log> the <version-message> for the <console> with <application-version>.

    <Return> an <OK: status> for the <handler>.
}
